<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="A1.Views.historydetails">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" VerticalOptions="Center">
            <Label TextColor="Black" x:Name="iname"></Label>
            <Label TextColor="Black" x:Name="iquant"></Label>
            <Label TextColor="Black" x:Name="idate"></Label>
            <Label TextColor="Black" x:Name="itotal"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>